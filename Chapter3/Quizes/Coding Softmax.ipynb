   "outputs": [],
   "source": [
    "def softmax(L):\n",
    "    res = []\n",
    "    for i in L:\n",
    "        val = (np.exp(i))/np.sum(np.exp(L))\n",
    "        res.append(val)\n",
    "    return res"
   ]
  },
